                    RowBox[{"jt", ",", 
                    RowBox[{"dim", "[", 
                    RowBox[{"[", "p", "]"}], "]"}]}], "}"}]}], "]"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"pt", ",", "pop"}], "}"}]}], "]"}], ",", "F"}], 
                    "]"}], ",", "1"}], "]"}], ",", "s", ",", 
